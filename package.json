{
  "name": "jquery.resizeend",
  "version": "1.0.1",
  "description": "A jQuery plugin that allows for window resize-end event handling.",
  "repository": {
    "type": "git",
    "url": "https://github.com/nielse63/jquery.resizeend"
  },
  "author": {
    "name": "Erik Nielsen",
    "email": "erik@312development.com",
    "url": "https://312development.com"
  },
  "license": {
    "type": "MIT",
    "url": "https://opensource.org/licenses/MIT"
  },
  "homepage": "https://github.com/nielse63/jquery.resizeend",
  "keywords": [
    "jquery-plugin",
    "resizeend",
    "jQuery",
    "events",
    "scroll"
  ],
  "bugs": {
    "url": "https://github.com/nielse63/jquery.resizeend/issues",
    "email": "erik@312development.com"
  },
  "main": "lib/jquery.resizeend.js",
  "files": [
    "lib/"
  ],
  "directories": {
    "lib": "./lib"
  },
  "scripts": {
    "lint": "eslint src/ test/ ./*.js --cache --fix",
    "build:dev": "cross-env NODE_ENV=development webpack -d",
    "build:prod": "cross-env NODE_ENV=production webpack -p",
    "build": "npm run build:dev && npm run build:prod",
    "postbuild": "cp lib/resizeend.min.js test/static/resizeend.min.js",
    "pretest": "npm run build:prod",
    "test": "jest",
    "coverage": "cat coverage/lcov.info | coveralls",
    "commitmsg": "validate-commit-msg",
    "precommit": "lint-staged",
    "prepush": "nsp check",
    "changelog": "github-changes -n ${npm_package_version} --only-pulls --use-commit-body --owner nielse63 --repository ${npm_package_name}",
    "version": "scripts/release"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.3",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-stage-2": "^6.24.1",
    "conventional-commit-types": "^2.2.0",
    "coveralls": "^3.0.0",
    "cross-env": "^5.1.2",
    "eslint": "^4.5.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-xo": "^0.19.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-flowtype": "^2.40.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-prettier": "^2.4.0",
    "express": "^4.16.2",
    "flow-bin": "^0.61.0",
    "github-changes": "^1.1.1",
    "husky": "^0.14.3",
    "jest": "^22.0.3",
    "jest-environment-node": "^22.0.3",
    "lint-staged": "^6.0.0",
    "mkdirp": "^0.5.1",
    "nsp": "^3.1.0",
    "prettier": "^1.9.2",
    "puppeteer": "^0.13.0",
    "rimraf": "^2.6.2",
    "staged-git-files": "^0.0.4",
    "validate-commit-msg": "^2.14.0",
    "webpack": "^3.10.0"
  },
  "engines": {
    "node": ">= 8.x",
    "npm": ">= 5.x",
    "yarn": ">= 1.x"
  },
  "config": {
    "validate-commit-msg": {
      "types": "conventional-commit-types",
      "subjectPatternErrorMsg": "[validate-commit-msg]: Subject does not match subject pattern!",
      "autoFix": true
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  }
}
